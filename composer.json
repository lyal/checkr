{
  "name": "wildfrustration/checkr",
  "description": "A PHP client for checkr.io, a background screening service - This is fork of lyal/checker",
  "keywords": [
    "laravel",
    "symphony",
    "checkr"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "wildfrustration",
      "email": "wildfrustration@yahoo.com"
    }
  ],
  "require": {
    "php": "8.0.*",
    "guzzlehttp/guzzle": "7.4.*",
    "illuminate/support": "9.18.0",
    "illuminate/routing": "9.18.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5.18",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Lyal\\Checkr\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Lyal\\Checkr\\Laravel\\CheckrServiceProvider"
      ],
      "aliases": {
        "Checkr": "Lyal\\Checkr\\Laravel\\Facades\\Checkr"
      }
    }
  }
}
